// Record value assignment with pointers
type
    RT = record
          x: int;
          y: int
         end;
    L = ^RT;
    RR = record
          x: int;
          y: int
        end;
    RC = RR;
var 
	
    r1: RT;
    r2: RR;
    r3: RC;
    p: L;
begin
  r1 := RT{ 4, 5 };
  write r1.y;
  write r1.x
end
